{
  "name": "fixedpronos-backend",
  "version": "1.0.0",
  "description": "Fixed Edge Pro backend built with Node.js, Express, Prisma, and PostgreSQL.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "promote-admin": "ts-node scripts/promoteAdmin.ts",
    "test": "vitest run"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.11",
    "pino": "^8.17.0",
    "pino-http": "^9.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^2.0.16",
    "pino-pretty": "^11.0.0",
    "prisma": "^5.15.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "license": "MIT",
  "author": "Fixed Edge Pro"
}

